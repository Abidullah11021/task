			</array>
		</dict>
	</array>

	<!-- ✅ Required for Google Sign-In -->
	<key>GIDClientID</key>
	<string>535423821816-n7vcqpqo3vjup6jbcnc2ab025ehfe9dk.apps.googleusercontent.com</string>

	<!-- ✅ Allow Google app/Safari queries -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>google</string>
		<string>com.google</string>
		<string>com.google.chrome.ios</string>
		<string>com.google.GPPShare</string>
		<string>com.google.GPPShareExtension</string>
	</array>
